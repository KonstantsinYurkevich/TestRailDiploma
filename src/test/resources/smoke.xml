<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Test suite" parallel="tests">
    <listeners>
        <listener class-name="tests.base.AnnotationTransformer"/>
    </listeners>
    <test name="UI Tests">
        <classes>
            <class name="tests.AddProjectTest"/>
            <class name="tests.LogInLogOutTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="API Tests">
        <classes>
            <class name="tests.api.AddProjectApiTest"/>
        </classes>
    </test> <!-- Test -->
</suite>
